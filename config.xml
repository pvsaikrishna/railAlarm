<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.labakshaya.trainalarm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Train Alarm</name>
  <description>
	An application to notify user when destination is about to reach, for a tense free and good sleep train journey.
  </description>
  <author email="labakshaya@gmail.com" href="http://ionicframework.com/">
      Team Akshaya
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
<platform name="android">
  <icon src="icons/android/36x36.png" width="36" height="36" />
  <icon src="icons/android/48x48.png" width="48" height="48" />
  <icon src="icons/android/64x64.png" width="64" height="64" />
  <icon src="icons/android/72x72.png" width="72" height="72" />
  <icon src="icons/android/96x96.png" width="96" height="96" />
</platform>
</widget>
