<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.labakshaya.trainalarm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Train Alarm</name>
  <description>
	An application to notify user when destination is about to reach, for a tense free and good sleep train journey.
  </description>
  <author email="labakshaya@gmail.com" href="http://ionicframework.com/">
      Team Akshaya
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
</widget>
